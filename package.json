  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.0.0-beta2",
    "aos": "^2.3.4",
    "bootstrap": "^5.1.2",
    "copy-files-from-to": "^3.2.2",
    "jquery": "^3.6.0"
  },
  "copyFiles": [
    {
      "from": "node_modules/jquery/dist/jquery.min.js",
      "to": "assets/js/jquery.min.js"
    },
    {
      "from": "node_modules/aos/dist/aos.css",
      "to": "assets/css/aos.css"
    },
    {
      "from": "node_modules/aos/dist/aos.js",
      "to": "assets/js/aos.js"
    },
    {
      "from": "node_modules/bootstrap/dist/js/bootstrap.bundle.min.js",
      "to": "assets/js/bootstrap.bundle.min.js"
    },
    {
      "from": "node_modules/bootstrap/dist/js/bootstrap.bundle.min.js.map",
      "to": "assets/js/bootstrap.bundle.min.js.map"
    },
    {
      "from": "node_modules/owl.carousel/dist/owl.carousel.min.js",
      "to": "assets/js/owl.carousel.min.js"
    },
    {
      "from": "node_modules/owl.carousel/dist/assets/owl.carousel.min.css",
      "to": "assets/css/owl.carousel.min.css"
    },
    {
      "from": "node_modules/owl.carousel/dist/assets/owl.theme.default.min.css",
      "to": "assets/css/owl.theme.default.min.css"
    },
    {
      "from": "node_modules/@fortawesome/fontawesome-free/js/all.min.js",
      "to": "assets/font/fontawesome/all.min.js"
    }
  ]
}
